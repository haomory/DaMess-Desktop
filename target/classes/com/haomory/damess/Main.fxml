   </ImageView>
   <Button fx:id="registerBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="216.0" layoutY="377.0" mnemonicParsing="false" prefHeight="69.0" prefWidth="288.0" style="-fx-background-color: #02D417; -fx-background-radius: 50px; -fx-padding: 5px;" text="Register" textAlignment="CENTER" textFill="WHITE">
